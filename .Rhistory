#Useful functions:
#pooled sd:
pooled_sd <- function(x,y)
{
n1<-length(x)
n2<-length(y)
s1<-sd(x)
s2<-sd(y)
pooled<-sqrt(((n1-1)*s1^2+(n2-1)*s2^2)/(n1+n2-2))
return(pooled)
}
effect_size<-function(x,y)
{
effect_size_calc<-abs(mean(x)-mean(y))/pooled_sd(x,y)
return(effect_size_calc)
}
